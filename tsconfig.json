{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"lib": [
			"es2015",
			"es2016",
			"es2017",
			"esnext"
		],
		"allowJs": false,
		"checkJs": false,
		"declaration": false,
		"declarationMap": false,
		"sourceMap": false,
		"outDir": "./build",
		"rootDir": "./src",
		"removeComments": false,
		"strict": false,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true
	}
}
